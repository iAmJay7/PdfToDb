
@{
    ViewBag.Title = "List";
}
@model IEnumerable<PdfToDb.Models.EmpDataViewModel>
<h3><a href="/Home/Index" class="btn btn-info">Back</a></h3>
<h2>List</h2>
@if (Model != null)
{
    <div class="container">
        <div class="row">
            <div class="col-lg-10">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Code</th>
                            <th>Name</th>
                            <th>Work Hours</th>
                            <th>Over Time</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var a in Model)
                        {
                            <tr>
                                <td>@a.Code</td>
                                <td>@a.Name</td>
                                <td>@a.WorkHours</td>
                                <td>@a.OverTime</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
}
